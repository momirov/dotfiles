URxvt*foreground: #CCCCCC
URxvt*background: #111111
!black
*color0: #222222
*color8: #666666
!red
*color1: #E84F4F
*color9: #D23D3D
!green
*color2: #B7CE42
*color10: #BDE077
!yellow
*color3: #FEA63C
*color11: #FFE863
!blue
*color4: #66A9B9
*color12: #AACCBB
!magenta
*color5: #B7416E
*color13: #E16A98
!cyan
*color6: #6D878D
*color14: #42717B
!white
*color7: #CCCCCC
*color15: #FFFFFF

Xft.dpi: 94
!Xft.autohint: 0
!Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
!Xft.hinting: 1
!Xft.antialias: 1
!Xft.rgba: rgb
Xft.antialias: 1
Xft.hinting: 1
!Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
!Xft.autohint: 0

! This is working terminus font
!URxvt.font:        -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
!URxvt.boldFont:    -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
URxvt.font: xft:Iosevka Term:style=Regular:size=12
URxvt.boldFont: xft:Iosevka Term:style=Regular:size=12
URxvt.italicFont: xft:Iosevka Term:style=Regular:size=12
URxvt.boldItalicFont: xft:Iosevka Term:style=Regular:size=12
!URxvt.font: xft:Tewi:size=8
!URxvt.font:             -lucy-tewi-medium-r-normal-*-11-90-75-75-p-58-iso10646-1
!URxvt.boldFont:         -lucy-tewi-bold-r-normal-*-11-90-75-75-p-58-iso10646-1

URxvt.scrollBar : false

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.url-select.launcher:  /usr/bin/xdg-open
URxvt.url-select.underline: true

URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.saveLines: 12000
URxvt.termName: rxvt
